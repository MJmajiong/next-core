{
  "name": "@next-core/brick-http",
  "version": "2.2.10",
  "description": "> TODO: description",
  "homepage": "https://github.com/easyops-cn/next-core/tree/master/packages/brick-http",
  "license": "GPL-3.0",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:easyops-cn/next-core.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prestart": "npm run clean",
    "start": "concurrently -k -n tsc,build \"npm run start:types\" \"npm run start:rollup\"",
    "start:rollup": "cross-env NODE_ENV='development' rollup --watch --config rollup.config.js",
    "start:types": "tsc --watch --emitDeclarationOnly --declaration --declarationDir dist/types",
    "prebuild": "npm run clean",
    "build": "npm run build:types && npm run build:main",
    "build:main": "cross-env NODE_ENV='production' build-next-libs",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "test": "cross-env NODE_ENV='test' jest",
    "test:ci": "cross-env NODE_ENV='test' CI=true jest"
  },
  "sideEffects": false,
  "devDependencies": {
    "@next-core/build-next-libs": "^0.0.0",
    "whatwg-fetch": "^3.6.2"
  }
}
